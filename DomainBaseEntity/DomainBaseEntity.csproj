<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<PackageId>MatinDeWet.DomainBaseEntity</PackageId>
	<Version>1.0.0</Version>
	<Authors>MatinDeWet</Authors>
	<Product>DomainBaseEntity</Product>
	<Description>DomainBaseEntity for .NET was designed to be used as a extension library when generating Database models using Entity Framework Core. It provides a base class that can be used to add common properties to all entities in a database model.</Description>
	<RepositoryUrl>https://github.com/MatinDeWet/DomainBaseEntity</RepositoryUrl>
	<PackageProjectUrl>https://github.com/MatinDeWet/DomainBaseEntity</PackageProjectUrl>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
	<Title>DomainBaseEntity</Title>
  </PropertyGroup>

	<ItemGroup>
		<None Include="..\LICENSE.txt">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
